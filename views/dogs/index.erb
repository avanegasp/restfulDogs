<h1>Estas son las razas: </h1>

<ul>
  <%@dogs.each do |dog| %>
    <li><%= dog.name %>
      <a href="/dogs/<%= dog.id %>/edit">Editar</a>
    </li>
    <li>
      <form action="/dogs/<%= dog.id %>" method="post">
        <input type="hidden" name="_method" value="delete">
        <input type="submit" value="Borrar raza">
      </form>
    </li>

  <% end%>
</ul>
